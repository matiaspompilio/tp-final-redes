!
frr version 7.3
frr defaults traditional
hostname n37
log syslog informational
service integrated-vtysh-config
!
interface eth0
 ip address 10.0.26.2/24
!
interface eth1
 ip address 10.0.25.1/24
!
interface eth2
 ip address 10.0.27.2/24
!
router bgp 64444
 neighbor 10.0.25.2 remote-as 64444
 neighbor 10.0.26.1 remote-as 64444
 neighbor 10.0.27.1 remote-as 65032
 !
 address-family ipv4 unicast
  network 10.0.0.0/8
  network 10.0.25.0/24
  network 10.0.26.0/24
  network 10.0.27.0/24
 exit-address-family
!
line vty
!
end
